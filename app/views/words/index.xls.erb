<?xml version="1.0"?>
<Workbook xmlns="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:o="urn:schemas-microsoft-com:office:office"
  xmlns:x="urn:schemas-microsoft-com:office:excel"
  xmlns:ss="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:html="http://www.w3.org/TR/REC-html40">
  <Worksheet>
    <Table>
      <Row>
        <Cell><Data ss:Type="String"><%= t "word.word" %></Data></Cell>
        <Cell><Data ss:Type="String"><%= t "word.mean" %></Data></Cell>
      </Row>
      <% @words.each do |word| %>
        <% word.answers.each do |answer| %>
          <% if answer.is_correct %>
            <Row>
              <Cell><Data ss:Type="String"><%= word.content %></Data></Cell>
              <Cell><Data ss:Type="String"><%= answer.content %></Data></Cell>
            </Row>
          <% end %>
        <% end %>
      <% end %>
    </Table>
  </Worksheet>
</Workbook>
